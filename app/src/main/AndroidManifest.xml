<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rlawk.tremproject3">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CallList" />
        <activity android:name=".MessageList" />
        <activity android:name=".MessagePerPerson" />
        <activity android:name=".PhoneList" />
        <activity android:name=".AddPerson" />
        <activity android:name=".CallListSingleItem" />
        <activity android:name=".MessageItem" />
        <activity android:name=".PhoneListItem" />
        <activity android:name=".SendMessage"></activity>
    </application>

</manifest>